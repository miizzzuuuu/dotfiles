  "tokenColors": [
    {
      "scope": ["emphasis"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["strong"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markup headings",
      "scope": ["header", "markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6c66fa"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "unused.comment",
        "wildcard.comment"
      ],
      "settings": {
        "foreground": "#5a5b7e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call.object",
        "meta.function-call.php",
        "meta.function-call.static",
        "meta.method-call.java meta.method",
        "meta.method.groovy",
        "support.function.any-method.lua",
        "keyword.operator.function.infix"
      ],
      "settings": {
        "foreground": "#5bc9ff"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#e1ac0e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "markup.inline.raw"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Constants & Numbers",
      "scope": [
        "constant",
        "variable.other.constant",
        "constant.numeric",
        "constant.character.numeric"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Classes & Types",
      "scope": [
        "entity.name.type.class",
        "entity.name.class",
        "entity.name.type",
        "storage.type.cs",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#6c66fa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#f8f8ff"
      }
    },
    {
      "name": "JSX/TSX Expression Braces",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.jsx",
        "punctuation.section.embedded.end.jsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "HTML/CSS Attribute names",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#50fa7b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "entity.name.variable.parameter"],
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "this, super, self, etc.",
      "scope": [
        "keyword.other.this",
        "variable.language",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "entity.name.tag.yaml",
        "variable.other.key.toml"
      ],
      "settings": {
        "foreground": "#5bc9ff"
      }
    },
    {
      "name": "Brackets & Punctuation",
      "scope": [
        "meta.brace",
        "punctuation.definition.arguments",
        "punctuation.definition.dict",
        "punctuation.section.scope"
      ],
      "settings": {
        "foreground": "#f8f8ff"
      }
    }
  ]
